<ruleset name="Cyberstreetwise">
    <target host="cyberstreetwise.com" />
    <target host="*.cyberstreetwise.com" />

    <securecookie host=".+" name=".+" />

    <rule from="^http://cyberstreetwise\.com/"
        to="https://www.cyberstreetwise.com/" />
    <rule from="^http:" to="https:" />
</ruleset>
