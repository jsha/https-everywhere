<ruleset name="Alternate" platform="mixedcontent">
  <target host="*.alternate.be"/>
  <target host="alternate.be"/>
  <target host="*.alternate.nl"/>
  <target host="alternate.nl"/>
  <target host="*.alternate-b2b.nl"/>
  <target host="alternate-b2b.nl"/>

  <rule from="^http:" to="https:"/>
  <rule from="^http://alternate\.be/" to="https://alternate.be/"/>
</ruleset>
