<ruleset name="ING">
    <target host="ing.com" />
    <target host="www.ing.com" />
    <target host="ing.nl" />
    <target host="mijn.ing.nl" />
    <target host="mijnzakelijk.ing.nl" />
    <target host="www.ing.nl" />

    <rule from="^http://ing\.com/"
        to="https://www.ing.com/" />
    <rule from="^http://ing\.nl/"
        to="https://www.ing.nl/" />
    <rule from="^http:" to="https:" />
</ruleset>
