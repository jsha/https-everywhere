<ruleset name="Ixquick">
  <target host="ixquick.com" />
  <target host="*.ixquick.com" />
  <target host="startpage.com" />
  <target host="*.startpage.com" />
  <target host="startingpage.com" />
  <target host="*.startingpage.com" />
  <target host="ixquick-proxy.com" />
  <target host="*.ixquick-proxy.com" />

  <rule from="^http:" to="https:" />

  <securecookie host="^\.ixquick\.com$" name=".*"/>

</ruleset>
